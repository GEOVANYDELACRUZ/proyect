<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MANAGE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_alumnopantal"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProyectoFinal"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".ui.editar.pass"
            android:exported="false" />
        <activity
            android:name=".ui.editar.datos"
            android:exported="false" />
        <activity
            android:name=".ui.recuperar.password"
            android:exported="false" />
        <activity android:name=".ui.asignarnota.AsignarNotasAlum" />
        <activity android:name=".ui.Registrar.registrar" />
        <activity
            android:name=".ui.IniciarSesion.iniciarsesion"
            android:exported="true" />
        <activity
            android:name=".ui.Splash.Splash"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ProyectoFinal.NoActionBar" />
    </application>

</manifest>